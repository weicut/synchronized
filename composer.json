{
  "name": "hyperf/synchronized",
  "description": "A synchronized for hyperf.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "synchronized"
  ],
  "homepage": "https://hyperf.io",
  "support": {
    "docs": "https://hyperf.wiki",
    "issues": "https://github.com/hyperf/hyperf/issues",
    "pull-request": "https://github.com/hyperf/hyperf/pulls",
    "source": "https://github.com/hyperf/hyperf"
  },
  "require": {
    "php": ">=7.2",
    "hyperf/contract": "~2.1.0",
    "hyperf/framework": "~2.1.0",
    "hyperf/utils": "~2.1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.9",
    "malukenho/docheader": "^0.1.6",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.4"
  },
  "suggest": {
    "hyperf/di": "Required to use annotation."
  },
  "autoload": {
    "psr-4": {
      "Hyperf\\Synchronized\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\Synchronized\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    },
    "hyperf": {
      "config": "Hyperf\\Synchronized\\ConfigProvider"
    }
  },
  "bin": [
  ],
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "phpunit --colors=always"
  }
}
